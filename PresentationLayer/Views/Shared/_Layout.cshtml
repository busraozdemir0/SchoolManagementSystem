
<!DOCTYPE html>
<html lang="en">

<head>
    @await Html.PartialAsync("/Views/_LayoutPartial/_HeaderPartial.cshtml")
</head>

<body>
    <!-- header -->
    @await Html.PartialAsync("/Views/_LayoutPartial/_NavbarPartial.cshtml")
    <!-- //header -->

    @RenderBody()

    <!-- footer block -->
    @await Html.PartialAsync("/Views/_LayoutPartial/_FooterPartial.cshtml")
    <!-- //footer block -->


    <!-- Js scripts -->
    @await Html.PartialAsync("/Views/_LayoutPartial/_ScriptsPartial.cshtml")
    <!-- //Js scripts -->

    @await RenderSectionAsync("Scripts", false)

    @await Component.InvokeAsync("NToastNotify")
</body>
</html>
